  "description": "SDK for automation testing with MongoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mybono/sdk_automation.git"
  },
  "publishConfig": {
